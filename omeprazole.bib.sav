  issue           = {4},
  keywords        = {Adult; Aged; Anti-Ulcer Agents, therapeutic use; Benzimidazoles, administration & dosage, therapeutic use; Female; Gastric Acid, metabolism; Humans; Male; Middle Aged; Omeprazole; Zollinger-Ellison Syndrome, drug therapy, metabolism},
