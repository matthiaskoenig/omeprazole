@Article{Walt1983,
  author          = {Walt, R. P. and Gomes, M. D. and Wood, E. C. and Logan, L. H. and Pounder, R. E.},
  journal         = {British medical journal (Clinical research ed.)},
  title           = {Effect of daily oral omeprazole on 24 hour intragastric acidity.},
  year            = {1983},
