  keywords        = {Adult; Double-Blind Method; Gastric Acid, metabolism; Gastric Mucosa, cytology, drug effects; Gastrins, blood; Humans; Male; Omeprazole, adverse effects, pharmacology; Reference Values},
  nlm-id          = {8707234},
