  issue           = {5},
  keywords        = {Administration, Oral; Adult; Anti-Ulcer Agents, metabolism; Benzimidazoles, administration & dosage, metabolism, pharmacology; Gastric Acid, metabolism; Humans; Kinetics; Male; Omeprazole},
